{
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@wdio/allure-reporter": "^9.0.4",
    "@wdio/cli": "^8.1.0",
    "@wdio/local-runner": "^8.1.0",
    "@wdio/mocha-framework": "^8.1.0",
    "@wdio/spec-reporter": "^8.1.0",
    "allure-commandline": "^2.30.0"
  },
  "scripts": {
    "report": "allure serve allure-results",
    "test:pixel": "npx wdio run configs/wdio.pixel3.conf.js",
    "test:samsung": "npx wdio run configs/wdio.samsungS22.conf.js",
    "test:pixel:drag": "npx wdio run configs/wdio.pixel3.conf.js --spec test/specs/drag.spec.js",
    "test:pixel:swipe": "npx wdio run configs/wdio.pixel3.conf.js --spec test/specs/swipe.spec.js",
    "test:pixel:signup": "npx wdio run configs/wdio.pixel3.conf.js --spec test/specs/signUp.spec.js",
    "test:samsung:drag": "npx wdio run configs/wdio.samsungS22.conf.js --spec test/specs/drag.spec.js",
    "test:samsung:swipe": "npx wdio run configs/wdio.samsungS22.conf.js --spec test/specs/swipe.spec.js",
    "test:samsung:signup": "npx wdio run configs/wdio.samsungS22.conf.js --spec test/specs/signUp.spec.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
